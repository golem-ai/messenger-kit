{
  "name": "golem-ai/messenger-kit",
  "type": "library",
  "homepage": "https://github.com/golem-ai/messenger-kit",
  "license": "MIT",
  "authors": [
    {
      "name": "Adrien Brault",
      "email": "adrien.brault@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "nesbot/carbon": "^2.0",
    "symfony/console": "^4.4 || ^5.0",
    "symfony/messenger": "^4.4 || ^5.0"
  },
  "require-dev": {
    "jangregor/phpstan-prophecy": "^1.0",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.1",
    "phpstan/phpstan-symfony": "^1.1",
    "phpunit/php-code-coverage": "^9.2",
    "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0",
    "symfony/framework-bundle": "^4.4 || ^5.0 || ^6.0",
    "symplify/easy-coding-standard": "^9.3"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "GolemAi\\MessengerKit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GolemAi\\MessengerKit\\Tests\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/golem-ai/messenger-kit/issues",
    "source": "https://github.com/golem-ai/messenger-kit"
  }
}
